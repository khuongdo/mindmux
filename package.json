{
  "name": "mindmux",
  "version": "0.1.0",
  "description": "Multi-Agent AI CLI for orchestrating AI agents",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "mux": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "dev:watch": "tsx watch src/cli.ts",
    "debug": "node --inspect-brk dist/cli.js",
    "debug:ts": "tsx --inspect-brk src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "volta": {
    "node": "24.2.0",
    "npm": "11.0.0"
  },
  "keywords": [
    "ai",
    "cli",
    "agents",
    "multi-agent",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "dotenv": "^17.2.3",
    "node-tmux": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
