{
  "name": "mindmux",
  "version": "0.1.0",
  "description": "Multi-Agent AI CLI for orchestrating AI agents",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "mux": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "dev:watch": "tsx watch src/cli.ts",
    "debug": "node --inspect-brk dist/cli.js",
    "debug:ts": "tsx --inspect-brk src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "docker:build": "docker build -f docker/Dockerfile -t mindmux:latest .",
    "docker:build-dev": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f mindmux",
    "prepare": "node -e \"try { require('husky').install() } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\""
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "volta": {
    "node": "24.2.0",
    "npm": "11.0.0"
  },
  "keywords": [
    "ai",
    "cli",
    "agents",
    "multi-agent",
    "orchestration"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mindmux"
  },
  "bugs": {
    "url": "https://github.com/yourusername/mindmux/issues"
  },
  "homepage": "https://github.com/yourusername/mindmux#readme",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "ansis": "^4.2.0",
    "better-sqlite3": "^9.2.2",
    "commander": "^13.1.0",
    "dotenv": "^17.2.3",
    "node-tmux": "^1.0.2",
    "pg": "^8.11.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
